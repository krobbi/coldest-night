# NightScript Version 3.
# Unlock Terminal Dialog
# English dialog implementation for the unlock terminal in test/area_bx/west.

func get_lock_option(){
	return "Lock annex";
}

func get_unlock_option(){
	return "Unlock annex";
}

func get_cancel_option(){
	return "Cancel";
}

func lock_prompt_dialog(){
	say("West annex is unlocked.{p=0.5}\nLock annex?");
}

func unlock_prompt_dialog(){
	say("West annex is locked.{p=0.5}\nUnlock annex?");
}

func locked_dialog(){
	say("Annex locked!");
}

func unlocked_dialog(){
	say("Annex unlocked!");
}
